<Window x:Class="ZPI_Paletyzator.View.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ZPI_Paletyzator.View"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid Height="419" VerticalAlignment="Top">
        <Button Command="{Binding Path=ChangeFibNumCommand}" Content="Button" HorizontalAlignment="Left" Margin="278,141,0,0" VerticalAlignment="Top" Width="232" Height="55"/>
        <TextBox Text="{Binding Path=FibNum}" HorizontalAlignment="Left" Height="126" Margin="464,10,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="318"/>
        <TextBox Text="{Binding Path=FibNumPrev}" HorizontalAlignment="Left" Height="126" Margin="10,10,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="318"/>

    </Grid>
</Window>
